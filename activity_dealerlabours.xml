                 android:layout_height="wrap_content"
                 android:gravity="center" >

                 <TextView
                     android:layout_width="25dp"
                     android:layout_height="30dp"
                     android:layout_weight="0.25" />

                 <LinearLayout
                     android:layout_width="wrap_content"
                     android:layout_height="wrap_content"
                      android:gravity="center" 
                     android:orientation="vertical" >
                     
                         <ImageButton
                     android:id="@+id/homeelectric"
                          android:layout_width="150dp"
                     android:layout_height="100dp"
                     
                     android:background="@drawable/homeelectric" />
                         
                            <TextView
                     android:id="@+id/electrician"
                     android:layout_width="wrap_content"
                     android:layout_height="wrap_content"
                     android:gravity="center"
                     android:text="Electricians"
                     android:textColor="#4A3701"
                     android:textSize="16sp" />
                 </LinearLayout>

              

             

              

                 <TextView
                     android:layout_width="25dp"
                     android:layout_height="30dp"
                     android:layout_weight="0.5" />

                 <LinearLayout
                     android:layout_width="wrap_content"
                     android:layout_height="wrap_content"
                     android:orientation="vertical"
                     android:layout_gravity="center"
                     android:gravity="center" >

                     <ImageButton
                         android:id="@+id/homeplumb"
                     android:layout_width="150dp"
                     android:layout_height="100dp"
                         android:background="@drawable/homeplumb" />
                     
                     <TextView
                     android:id="@+id/plumbers"
                     android:layout_width="wrap_content"
                     android:layout_height="wrap_content"
                     android:gravity="center"
                     android:text="Plumbers"
                     android:textColor="#4A3701"
                     android:textSize="16sp" />

                    
                 </LinearLayout>

                 <LinearLayout
                     android:layout_width="wrap_content"
                     android:layout_height="wrap_content"
                      android:gravity="center" 
                     android:orientation="vertical" >

                 </LinearLayout>

                 <TextView
                     android:layout_width="25dp"
                     android:layout_height="30dp"
                     android:layout_weight="0.25" />
             </TableRow>
          
            
            <TableRow
                android:layout_width="match_parent"
                android:layout_height="10dp" >
            
           </TableRow>
           
             <TableRow
                 android:layout_width="match_parent"
                 android:layout_height="wrap_content"
                 android:gravity="center" >

                 <TextView
                     android:layout_width="25dp"
                     android:layout_height="30dp"
                     android:layout_weight="0.25" />

                 <LinearLayout
                     android:layout_width="wrap_content"
                     android:layout_height="wrap_content"
                      android:gravity="center" 
                     android:orientation="vertical" >
                     
                         <ImageButton
                     android:id="@+id/pestclean"
                          android:layout_width="150dp"
                     android:layout_height="100dp"
                     
                     android:background="@drawable/pest" />
                         
                            <TextView
                     android:id="@+id/pest"
                     android:layout_width="wrap_content"
                     android:layout_height="wrap_content"
                     android:gravity="center"
                     android:text="Pest Cleaner"
                     android:textColor="#4A3701"
                     android:textSize="16sp" />
                 </LinearLayout>

              

             

              

                 <TextView
                     android:layout_width="25dp"
                     android:layout_height="30dp"
                     android:layout_weight="0.5" />

                 <LinearLayout
                     android:layout_width="wrap_content"
                     android:layout_height="wrap_content"
                     android:orientation="vertical"
                     android:layout_gravity="center"
                     android:gravity="center" >

                     <ImageButton
                         android:id="@+id/agriculture"
                     android:layout_width="150dp"
                     android:layout_height="100dp"
                         android:background="@drawable/agri" />
                     
                     <TextView
                     android:id="@+id/agri"
                     android:layout_width="wrap_content"
                     android:layout_height="wrap_content"
                     android:gravity="center"
                     android:text="Agriculture"
                     android:textColor="#4A3701"
                     android:textSize="16sp" />

                    
                 </LinearLayout>

                 <LinearLayout
                     android:layout_width="wrap_content"
                     android:layout_height="wrap_content"
                      android:gravity="center" 
                     android:orientation="vertical" >

                 </LinearLayout>

                 <TextView
                     android:layout_width="25dp"
                     android:layout_height="30dp"
                     android:layout_weight="0.25" />
             </TableRow>
          
            
            
            
          

          
            
        </LinearLayout>
    </ScrollView>
     <LinearLayout
        android:id="@+id/linearLayout3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:orientation="vertical" > 
    
        <TableRow
                
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                   >
            
            <Button 
		    android:id="@+id/wallet"
		    android:layout_width="400dp"
		    android:layout_height="50dp"
		    android:text="Show-Wallet"
		    android:textSize="16sp"
		    android:textStyle="bold"
		    android:textColor="#4A3701"
		   
		    
		    />

           

   


        </TableRow>
         
     </LinearLayout>
   


</RelativeLayout>
